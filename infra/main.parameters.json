{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "useKeyVault": {
      "value": "${USE_KEY_VAULT}"
    },
    "azureAdTenantId":{
      "value": "${AZURE_AD_TENANTID}"
    },
    "frontendClientId":{
      "value": "${FRONTEND_CLIENTID}"
    },
    "webApiClientId":{
      "value": "${WEBAPI_CLIENTID}"
    },
    "memoryStore":{
      "value": "${MEMORY_STORE=AzureCognitiveSearch}"
    },
    "deployCosmosDB":{
      "value": "${DEPLOY_COSMOSDB=false}"
    },
    "deploySpeechServices":{
      "value": "${DEPLOY_SPEECH_SERVICES=false}"
    },
    "deployWebSearcherPlugin":{
      "value": "${DEPLOY_WEB_SEARCHER_PLUGIN=false}"
    }
  }
}